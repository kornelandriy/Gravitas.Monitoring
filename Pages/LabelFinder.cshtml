@page
@model Gravitas.Monitoring.Pages.LabelFinderModel
@using Gravitas.Monitoring.HelpClasses
@using Gravitas.Monitoring.Constants
@inject SignInManager<IdentityUser> SignInManager
@{
	string tc = HttpContext.Request.Query["tc"].ToString();
	string Device = HttpContext.Request.Query["Device"].ToString();

	LabelFinderHelper lfh = new LabelFinderHelper();
	string Devices = "<fieldset><br />";
	foreach (string[] s in lfh.DeviceList)
	{

		if (s[0] == Device)
		{
			Devices += s[1] +"checked"+ s[2];
		}
		else
		{
			Devices += s[1] + s[2];
		}

	}
	Devices += "</fieldset>";



}
@Html.Raw(Devices)